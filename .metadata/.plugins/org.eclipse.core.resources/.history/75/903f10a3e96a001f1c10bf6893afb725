package java_Revision;

public class Array_Pro {

	public static void main(String[] args) 
	{
		int serialNo[]=new int [3];
		serialNo[0]=10;
		serialNo[1]=20;
		serialNo[2]=40;
		
		for(int i=0;i<=2;i++)
		{
			System.out.println(serialNo[i]);
		}
				
		String car[]= {"BMW","Maruti","Tata"};
		System.out.println(car[1]);

	}

}
