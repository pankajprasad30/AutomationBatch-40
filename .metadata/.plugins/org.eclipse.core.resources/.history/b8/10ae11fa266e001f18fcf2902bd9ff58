package testNG_Revision;

import org.testng.annotations.Test;

public class Annotation_Priority {

	@Test(priority=1)
	public void add()
	{
		System.out.println("Pankaj1");
	}
	@Test
	public void add2()
	{
		System.out.println("Pankaj2");
	}
	@Test(priority=-3)
	public void add3()
	{
		System.out.println("Pankaj3");
	}
	@Test 
	public void add4()
	{
		System.out.println("Pankaj4");
	}

}
